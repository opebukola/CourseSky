<div class="questionnav">
	<ol>
		<% @lesson.questions.order(:position).each do |q| %>
		<li id="questionanchor">
			<% if current_user %>
				<% if current_user.has_answered(q) %>
					<%= link_to q.objective + ' (completed)', anchor: 'question_' + q.position.to_s %>
				<% else %>
					<%= link_to q.objective, anchor: 'question_' + q.position.to_s %>
				<% end %>
			<% else %>
				<%= link_to q.objective, anchor: 'question_' + q.position.to_s %>
			<% end %>
		</li>
		<% end %>
	</ol>
</div>