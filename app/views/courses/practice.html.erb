<%=provide(:title, @course.title) %>
<%= render 'shared/course_intro' %>
<div class="skills-listing">
	<h3> Coming Soon! Master every <%= @course.title %> skill with unlimited practice questions.</h3><br />
	<h3> Skills Listing </h3><br />
	<% @course.skills.each do |s| %>
		<ul>
			<li><%= link_to s.description, new_quiz_path(course_id: @course)%></li>
		</ul>
	<% end %>
</div>