<%= provide(:title, 'All Users') %>
<ul>
<% @users.each do |user| %>
  <li>
    <%= user.email %> |
    <%= user.name %> |
    <%= link_to 'Delete User', user_path(user),
                                method: :delete,
                                data: {confirm: 'Are you sure?'} %>
  </li>
<% end %>
</ul>
